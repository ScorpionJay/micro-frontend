{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@config/*": ["config/*"],
      "@components/*": ["src/components/*"],
      "@utils/*": ["src/utils/*"],
      "@static/*": ["src/static/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
